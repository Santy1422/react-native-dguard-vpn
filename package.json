{
  "name": "react-native-dguard-vpn",
  "version": "1.1.1",
  "description": "DGuard VPN - React Native library for secure VPN connection with ATOM SDK integration and Expo Development Builds support",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index.tsx",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "react-native-dguard-vpn.podspec"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "clean": "del-cli lib"
  },
  "keywords": [
    "react-native",
    "vpn",
    "dguard",
    "security",
    "privacy",
    "expo",
    "ios",
    "android",
    "atom-sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/username/react-native-dguard-vpn.git"
  },
  "author": "DGuard Team <contact@dguard.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/username/react-native-dguard-vpn/issues"
  },
  "homepage": "https://github.com/username/react-native-dguard-vpn#readme",
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.70.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.0.0",
    "react": "^18.0.0",
    "react-native": "^0.70.0",
    "react-native-builder-bob": "^0.20.0",
    "typescript": "^4.8.0",
    "del-cli": "^5.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
